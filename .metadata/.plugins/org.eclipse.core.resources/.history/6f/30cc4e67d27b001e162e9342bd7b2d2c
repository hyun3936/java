package Chap_06; // 객체지향 구현

public class Armor { // 게터와 세터
	
	private String name;
	private int height;
	
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {  // void라 return반환값 없음.
		this.name = name;
	}
	
	
	
	public int getHeight() {
		return height;
	}
	
	public void setHeight(int height) {  // void라 return반환값 없음.
		this.height = height;
	}
}
