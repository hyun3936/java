package com.ruby.java.test1;

public class Armor {
	private String name;
	private int height;
	private int weight;
	private String color;
	private boolean isFly;
	public void setName() {
		String value = "mark6"; 
		name = value;
	}
	public void setHeight(int value) {
		height = value;
	}
	void takeOff() {
		System.out.println("비행하다");
		return;
		System.out.println("아이언맨");
	}
}