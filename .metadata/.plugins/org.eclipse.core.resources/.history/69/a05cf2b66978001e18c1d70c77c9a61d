package Chap_09;

import java.util.Random;

public class Test15 {
	public static void main(String[] args) {

		int[] data = new int[6]; // 배열 선언

		Random r2 = new Random();

		for (int i = 1; i < 7; i++) {
			data[i] = r2.nextInt(45);

		}
		for (int num : data)
			System.out.print(" " + num);
	}
}
