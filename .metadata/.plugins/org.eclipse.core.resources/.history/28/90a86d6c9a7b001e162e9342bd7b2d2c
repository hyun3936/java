package Chap_03;

public class Test20 {

	public static void main(String[] args) {
		int score = 90;
		int bonus;
		String result = "";
		
		if (score < 100) {
			bonus = 10;
		}else if (score < 200) {  //100<score & score < 200
			bonus = 20;
		}
		else
			bonus = 30;
		score += bonus;
		System.out.println(score);

	}

}
