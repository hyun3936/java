package Chap_13; // 제너릭 인터페이스

import java.util.Iterator;

interface Maximum <T extends Comparable<T>> implements Maximum<T> {
	T max();
	
	
	class NumUtil<T extends Comparable<T>> implements Maximum<T> {
	T[] value;
	
	
	NumUtil(T[] value){
		this.value = value;
	}
	
	
	public T max() {
		T v = value[0];
		
		for (int i = 0; i < value.length; i++) {
			if(value[i].compareTo(v) > 0) v = value[i];
		}
	}
	}
	
}









public class GenInterfaceTest {
	
}
