package Chap_13;

 class Bag(T) {
	 private T thing;
	 
	 public Bag(T thing) {
		 this.thing = thing;
	 }
	 public T getThing() {
		 return thing;
 }
	 
	 public void setThing(T thing) {
		 this.thing = thing;
	 }
	 
	 void showType() {
		 System.out.println("T의 타입은 " + thing.getClass().getName());
		 }
	 }






public class BagTest {

}
